
<form action="birth" method="post">
    Birth ID: <input type="text" name="birthId" /><br/>
    Hospital:
    <select name="hospitalName">
        <option>City Hospital</option><option>Govt Hospital</option>
    </select><br/>
    Father Name: <input type="text" name="fatherName" /><br/>
    Mother Name: <input type="text" name="motherName" /><br/>
    Date and Time: <input type="text" name="dateTime" /><br/>
    Doctor Name: <input type="text" name="doctorName" /><br/>
    Nurse Name: <input type="text" name="nurseName" /><br/>
    Hospital Type:
    <select name="hospitalType">
        <option>Private</option><option>Government</option>
    </select><br/>
    <input type="submit" value="Submit"/>
</form>