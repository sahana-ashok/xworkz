<form action="pass" method="post">
    Name: <input type="text" name="applicantName" /><br/>
    Aadhar No: <input type="text" name="aadharNo" /><br/>
    Address: <input type="text" name="address" /><br/>
    PAN No: <input type="text" name="panNo" /><br/>
    Country: <input type="text" name="country" /><br/>
    State: <input type="text" name="state" /><br/>
    City: <input type="text" name="city" /><br/>
    Pincode: <input type="text" name="pinCode" /><br/>
    Passport Type:
    <select name="passportType">
        <option>Normal</option><option>Tatkal</option>
    </select><br/>
    Payment Ref No: <input type="text" name="paymentRefNo" /><br/>
    <input type="submit" value="Apply"/>
</form>